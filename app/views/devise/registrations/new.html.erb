<div class="login">  
  
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= render "devise/shared/error_messages", resource: resource %>
  
    <div class="field">
      <h2 class="login_title">新規登録</h2>
      <%= f.label :"会社名" %><br />
      <%= f.text_field :companyname, autofocus: true, autocomplete: "companyname" %>
    </div>
    
    <div class="field">
      <%= f.label :"部署名" %><br />
      <%= f.text_field :departmentname, autofocus: true, autocomplete: "departmentname" %>
    </div>
  
  
    <div class="field">
      <%= f.label :"名前" %><br />
      <%= f.text_field :name, autofocus: true, autocomplete: "name" %>
    </div>
  
    <div class="field">
      <%= f.label :email %><br />
      <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
    </div>
  
    <div class="field">
      <%= f.label :password %>
      <% if @minimum_password_length %>
      <em>(<%= @minimum_password_length %> characters minimum)</em>
      <% end %><br />
      <%= f.password_field :password, autocomplete: "new-password" %>
    </div>
  
    <div class="field">
      <%= f.label :password_confirmation %><br />
      <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
    </div>
    
    <div class="field">
      <%= f.label :"グループ名(半角英数)" %><br />
      <%= f.text_field :group_key, autocomplete: "group_name" %>
    </div>
    
  
    <div class="actions">
      <%= f.submit "Sign up" %>
    </div>
  <% end %>
  
  <%= render "devise/shared/links" %>
</div>